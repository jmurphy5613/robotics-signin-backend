// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Events {
    id Int @id @default(autoincrement())
    startDate DateTime
    endDate DateTime
    title String
    code Int
    description String
    attendees User[]
}

model User {
    id Int @id @default(autoincrement())
    firstName String
    lastName String
    email String
    eventsAttended Events[]
}